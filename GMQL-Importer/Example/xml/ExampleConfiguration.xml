<root>
    <settings>
        <output_folder>GMQL-Importer/Example</output_folder>
        <download_enabled>FALSE</download_enabled>
        <transform_enabled>TRUE</transform_enabled>
        <load_enabled>TRUE</load_enabled>
    </settings>
    <source_list>
        <source name="TCGA2BED">
            <url>bioinf.iasi.cnr.it</url>
            <output_folder>GMQL-Importer/Example/TCGA2BED</output_folder>
            <gmql_user>public</gmql_user>
            <downloader>it.polimi.genomics.importer.DefaultImporter.FTPDownloader</downloader>
            <transformer>it.polimi.genomics.importer.DefaultImporter.NULLTransformer</transformer>
            <download_enabled>FALSE</download_enabled>
            <transform_enabled>FALSE</transform_enabled>
            <load_enabled>FALSE</load_enabled>
            <parameter_list>
                <parameter>
                    <key>username</key>
                    <value>anonymous</value>
                </parameter>
                <parameter>
                    <key>password</key>
                    <value/>
                </parameter>
            </parameter_list>
            <dataset_list>
                <dataset name="cnv">
                    <output_folder>ExampleCNV</output_folder>
                    <schema location="LOCAL">GMQL-Importer/Example/schemas/cnv.schema</schema>
                    <download_enabled>FALSE</download_enabled>
                    <transform_enabled>FALSE</transform_enabled>
                    <load_enabled>FALSE</load_enabled>
                    <parameter_list>
                        <parameter>
                            <key>folder_regex</key>
                            <value>^/bed/.*/cnv</value>
                        </parameter>
                        <parameter>
                            <key>files_regex</key>
                            <value>.*10A-01D-A393-01\.bed(\.meta)?$</value>
                        </parameter>
                    </parameter_list>
                </dataset>
                <dataset name="dnaseq">
                    <output_folder>ExampleDNASEQ</output_folder>
                    <schema location="LOCAL">GMQL-Importer/Example/schemas/dnaseq.schema</schema>
                    <download_enabled>FALSE</download_enabled>
                    <transform_enabled>FALSE</transform_enabled>
                    <load_enabled>FALSE</load_enabled>
                    <parameter_list>
                        <parameter>
                            <key>folder_regex</key>
                            <value>^/bed/.*/dnaseq</value>
                        </parameter>
                        <parameter>
                            <key>files_regex</key>
                            <value>.*11D-A30X-08\.bed(\.meta)?$</value>
                        </parameter>
                    </parameter_list>
                </dataset>
            </dataset_list>
        </source>
        <source name="ENCODE">
            <url>https://www.encodeproject.org/</url>
            <output_folder>ENCODE</output_folder>
            <gmql_user>public</gmql_user>
            <downloader>it.polimi.genomics.importer.ENCODEImporter.ENCODEDownloader</downloader>
            <transformer>it.polimi.genomics.importer.ENCODEImporter.ENCODETransformer</transformer>
            <download_enabled>TRUE</download_enabled>
            <transform_enabled>TRUE</transform_enabled>
            <load_enabled>TRUE</load_enabled>
            <parameter_list>
                <parameter>
                    <key>metadata_prefix</key>
                    <value>metadata/</value>
                </parameter>
                <parameter>
                    <key>metadata_suffix</key>
                    <value>/metadata.tsv</value>
                </parameter>
                <parameter>
                    <key>json_prefix</key>
                    <value>experiments/</value>
                </parameter>
                <parameter>
                    <key>json_suffix</key>
                    <value>/?frame=embedded&amp;format=json/</value>
                </parameter>
                <parameter>
                    <key>encode_metadata_configuration</key>
                    <value>GMQL-Importer/Example/xml/encodeMetadataConfig.xml</value>
                </parameter>
            </parameter_list>
            <dataset_list>
                <dataset name="exampleNarrowPeak">
                    <output_folder>ExampleNARROWPEAK</output_folder>
                    <schema location="LOCAL">GMQL-Importer/Example/schemas/narrowPeak.schema</schema>
                    <download_enabled>TRUE</download_enabled>
                    <transform_enabled>TRUE</transform_enabled>
                    <load_enabled>TRUE</load_enabled>
                    <parameter_list>
                        <parameter>
                            <key>type</key>
                            <value>Experiment</value>
                        </parameter>
                        <parameter>
                            <key>files.file_type</key>
                            <value>bed+narrowPeak</value>
                        </parameter>
                        <parameter>
                            <key>organ_slims</key>
                            <value>liver</value>
                        </parameter>
                        <parameter>
                            <key>replicates.library.biosample.biosample_type</key>
                            <value>primary+cell</value>
                        </parameter>
                        <parameter>
                            <key>assay_slims</key>
                            <value>DNA+accessibility</value>
                        </parameter>
                    </parameter_list>
                </dataset>
            </dataset_list>
        </source>
    </source_list>
</root>